alias svi='sudo vim'
alias vi=vim
alias g=git
alias gs='git status'
alias gc='git commit -am'
alias push='git push'
alias pull='git pull'
alias cob='git checkout -b'
alias npmr='npm run'
alias bashrc='. ~/.bashrc'
alias vibash='vi ~/.bashrc'
alias ll='ls -l'

export PATH="$(yarn global bin):$PATH"
export PATH="~/.npm-packages:$PATH"

alias master='co master'
alias integration='co integration'
alias co='checkoutAndPull'
function checkoutAndPull() {
  git checkout "$1"
  git pull
}

alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'

alias dimScreen=./dimScreen.sh
alias brightenScreen=./brightnenScreen.sh

